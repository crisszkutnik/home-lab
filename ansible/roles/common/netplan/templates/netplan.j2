network:
  version: 2
  renderer: NetworkManager
  ethernets:
    {{ default_interface_name }}:
      dhcp4: no
      dhcp6: no
      addresses:
        - {{ target_host_ip }}/24
        - {{ target_host_cloudflare_ip }}/24
      routes:
        - to: default
          via: 192.168.0.1
      nameservers:
        addresses: [1.1.1.1, 8.8.8.8, 8.8.8.4]
